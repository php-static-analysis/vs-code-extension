{
    "name": "php-static-analysis/vs-code-extension",
    "description": "Visual Studio Code extension to parse PHP static analysis attributes",
    "type": "visual-studio-code-extension",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpStaticAnalysis\\VsCodeExtension\\": "example/"
        }
    },
    "authors": [
        {
            "name": "Carlos Granados",
            "email": "carlos@fastdebug.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "php-static-analysis/phpstan-extension": "dev-main",
        "phpstan/extension-installer": "^1.3",
        "vimeo/psalm": "^5",
        "php-static-analysis/psalm-plugin": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "psalm": "psalm",
        "phpstan": "phpstan analyse"
    }
}
